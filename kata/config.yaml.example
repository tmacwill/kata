database:
  name: YOUR_DATABASE_NAME
  host: localhost
  password: YOUR_DATABASE_PASSWORD
  pool_size: 10
  port: 5432
  user: YOUR_DATABASE_USER

cache:
  memcached:
    type: memcached
    prefix: YOUR_KEY_PREFIX
    hosts:
      - 'localhost:11211'
      - 'localhost:11212'
  memory:
    type: memory
  redis:
    type: redis
    db: 0
    host: 'localhost:6379'
    prefix: YOUR_KEY_PREFIX

stats:
  host: localhost
  port: 8125
  prefix: YOUR_KEY_PREFIX
